<#import "common/spring.ftl" as s /><!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd"><html lang="en" class="no-js">    <head>        <base id="contextPath" href="${request.contextPath}/">        <meta charset="utf-8">        <title>微信公众号管理平台</title>        <meta name="viewport" content="width=device-width, initial-scale=1.0">        <meta name="description" content="">        <meta name="author" content="">        <link rel="shortcut icon" href="<@s.url '/img/favicon.ico'/>" type="image/x-icon">        <link rel="stylesheet" type="text/css" href="css/login/reset.css">        <link rel="stylesheet" type="text/css" href="css/login/supersized.css">        <link rel="stylesheet" type="text/css" href="css/login/style.css">    </head>    <body>        <div class="page-container">            <h1>微信公众号管理平台</h1>            <form id="loginform" action="" method="post">                <input type="text" id="username" name="username" class="username" placeholder="请输入您的用户名"/>                <input type="password" id="password" name="password" class="password" placeholder="请输入您的用户密码"/>                <input type="Captcha"  id="code" name="code" class="code"  placeholder="请输入验证码"/>				<img id="vcode" class="vcode" name="vcode" alt="验证码图片" title="看不清，换一张" src="getvalidatecode"/>                <button type="button" id="loginbtn" class="submit_button">登录</button>                <div id="error" class="error" style="margin-right:-30px;">                    <span>                    </span>                </div>            </form>            <div class="connect">                <p></p>            </div>        </div>        <script type="text/javascript" src="js/jquery-2.1.3.min.js" ></script>        <script type="text/javascript" src="Yui/jquery-1.8.0.min.js"></script>        <script type="text/javascript" src="js/login/supersized.3.2.7.min.js" ></script>        <script type="text/javascript" src="js/login/supersized-init.js" ></script>        <script type="text/javascript" src="js/layer/layer.min.js" ></script>        <script type="text/javascript" src="js/login/login.js" ></script>    </body></html>